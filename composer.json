{
	"name": "omnicode/lara-model",
	"description": "Useful model methods",
	"keywords": ["laravel", "bootstrapping", "model"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.5.*",
		"cakephp/utility": "~3.3",
		"omnicode/lara-support": "~2.0"
	},
	"require-dev": {
		"mockery/mockery": "~1.0",
		"phpunit/phpunit": "~6.0"
	},
	"suggest": {
		"omnicode/lara-crud": "LaraModel integrated with LaraCrud",
		"omnicode/lara-service": "LaraModel integrated with LaraService",
		"omnicode/lara-repo": "LaraModel integrated with LaraRepo"
	},
	"autoload": {
		"psr-4": {
			"LaraModel\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
